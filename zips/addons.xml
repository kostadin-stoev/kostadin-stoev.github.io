<?xml version="1.0" encoding="UTF-8"?>
<addons>
    <addon id="repository.aeon" name="Aeon Repository" version="1.0.0" provider-name="Aeon">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="Aeon Repository">
            <info compressed="false">https://raw.githubusercontent.com/kostadin-stoev/kostadin-stoev.github.io/main/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/kostadin-stoev/kostadin-stoev.github.io/main/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/kostadin-stoev/kostadin-stoev.github.io/main/zips/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Aeon's Repository</summary>
            <description>KODI Addons</description>
            <platform>all</platform>
    </extension>
</addon>

    <addon id="repository.mar33" name="Mar33 Repository" version="0.0.33" provider-name="MartinStz">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="Mar33 Repository">
            <info compressed="false">http://petrovy.com/_repo/addons.xml</info>
            <checksum>http://petrovy.com/_repo/addons.xml.md5</checksum>
            <datadir zip="true">http://petrovy.com/_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Repo from MartinStz</summary>
            <description>For installing Bulsat, Exodus, Covenant, Specto and subtitles addons</description>
            <platform>all</platform>
